{
  "expo": {
    "name": "FounderOS",
    "slug": "founder-os",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "founderos",
    "platforms": ["ios", "android", "web"],
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-router", "expo-secure-store"],
    "extra": {
      "eas": {
        "projectId": "founder-os"
      },
      "router": {
        "origin": false
      }
    }
  }
}

